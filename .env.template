# Server Configuration
NODE_ENV=development
PORT=5000

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=generate_a_strong_password_here
DB_NAME=celestial

# Email Configuration (Resend)
RESEND_API_KEY=replace_with_your_resend_api_key
VERIFICATION_EMAIL_URL=http://localhost:3000/verify-email
FRONTEND_URL=http://localhost:3000

# PayPal Configuration
PAYPAL_CLIENT_ID=replace_with_your_paypal_client_id
PAYPAL_CLIENT_SECRET=replace_with_your_paypal_client_secret

# Google OAuth
GOOGLE_CLIENT_ID=replace_with_your_google_client_id
GOOGLE_CLIENT_SECRET=replace_with_your_google_client_secret
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# Session Security
SESSION_SECRET=generate_a_long_random_string_here

# Backend URL
BACKEND_URL=http://localhost:5000

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=debug

# Database Connection URL
DATABASE_URL=postgres://postgres:your_secure_password@localhost:5432/celestial-sphere

# API Keys (REPLACE ALL OF THESE)
REACT_APP_ALPHA_VANTAGE_API_KEY=replace_with_alpha_vantage_api_key
REACT_APP_TWITCH_CLIENT_ID=replace_with_twitch_client_id
REACT_APP_TWITCH_CLIENT_SECRET=replace_with_twitch_client_secret
REACT_APP_INSTAGRAM_CLIENT_ID=replace_with_instagram_client_id
REACT_APP_INSTAGRAM_CLIENT_SECRET=replace_with_instagram_client_secret
REACT_APP_YOUTUBE_API_KEY=replace_with_youtube_api_key
REACT_APP_YOUTUBE_CLIENT_ID=replace_with_youtube_client_id
REACT_APP_YOUTUBE_CLIENT_SECRET=replace_with_youtube_client_secret
REACT_APP_TIKTOK_CLIENT_KEY=replace_with_tiktok_client_key
REACT_APP_TIKTOK_CLIENT_SECRET=replace_with_tiktok_client_secret

# Weather and News API Keys
WEATHER_API_KEY=replace_with_openweathermap_api_key
NEWS_API_KEY=replace_with_newsapi_key
